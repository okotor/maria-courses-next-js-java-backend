#AWS Secrets Manager
#aws secretsmanager create-secret --name marian-courses-java --secret-string file://.env.local
#aws secretsmanager get-secret-value --secret-id marian-courses-java --query SecretString --output text > .env.local
#aws secretsmanager update-secret --secret-id marian-courses-java --secret-string file://.env.local
#aws secretsmanager delete-secret --secret-id marian-courses-java